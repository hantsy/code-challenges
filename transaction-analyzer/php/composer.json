{
  "name": "hantsy/transaction-analyzer-php",
  "description": "A simple transaction analyzer implemented in PHP 8.0",
  "type": "project",
  "require": {
    "php": "^8.1",
    "brick/math": "^0.10.0",
    "brick/date-time": "^0.4.0",
    "ext-readline": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "classmap": [
      "src/"
    ],
    "psr-4": {
      "TransactionAnalyser\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": { "TransactionAnalyser\\Tests\\": "tests/" }
  },
  "scripts": {
    "start": [
      "TransactionAnalyser\\Main::run"
    ],
    "test": [
      "phpunit tests"
    ]
  },
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "hantsy",
      "email": "hantsy@gmail.com"
    }
  ]
}
